  "code": "{\n  \"simulationShader\": \"precision highp float;\\n\\nuniform sampler2D tPosition;\\nuniform float iTime;\\nuniform float iTimeDelta;\\nuniform vec2 iResolution;\\nuniform vec4 iMouse;\\n\\n// Hash function for randomness\\nfloat hash(vec2 p) {\\n    return fract(sin(dot(p, vec2(12.9898, 78.233))) * 43758.5453123);\\n}\\n\\nvec3 hash3(vec2 p) {\\n    return vec3(hash(p), hash(p + 1.5), hash(p + 3.0)) * 2.0 - 1.0;\\n}\\n\\nvoid main() {\\n    vec2 uv = gl_FragCoord.xy / iResolution.xy;\\n    vec4 data = texture2D(tPosition, uv);\\n    \\n    vec3 pos = data.xyz;\\n    float life = data.a;\\n\\n    // Parameters\\n    float decay = 0.4 + hash(uv * 1.5) * 0.3;\\n    float gravity = -1.2;\\n    float burstInterval = 3.0;\\n    \\n    // Determine a unique ID for the burst based on floor of time\\n    float burstID = floor(iTime / burstInterval);\\n    // Offset per particle index to make them burst at slightly different times or patterns\\n    float individualSeed = hash(uv);\\n    \\n    // Reset / Respawn Logic\\n    if (life <= 0.0 || iTime < 0.1) {\\n        // Random center for the firework\\n        vec3 origin = vec3(\\n            (hash(vec2(burstID, 0.1)) - 0.5) * 10.0,\\n            (hash(vec2(burstID, 0.2)) * 5.0) + 2.0,\\n            (hash(vec2(burstID, 0.3)) - 0.5) * 5.0\\n        );\\n        \\n        // If mouse is down, burst at mouse\\n        if(iMouse.z > 0.0) {\\n            origin = vec3((iMouse.x / iResolution.x - 0.5) * 20.0, (iMouse.y / iResolution.y - 0.5) * 20.0, 0.0);\\n        }\\n\\n        pos = origin;\\n        life = 1.0 + individualSeed; // Life starts at 1.0 (plus variation)\\n    } else {\\n        // Physics Logic\\n        // Calculate direction based on UV seed (this remains constant for the particle's life)\\n        vec3 dir = normalize(hash3(uv));\\n        float speed = 6.0 * (1.0 + hash(uv + 0.5));\\n        \\n        // Instead of Euler integration (pos += vel), we use a parametric equation relative to life\\n        // This prevents floating point drift in ping-pong buffers for this specific effect\\n        float t = (1.0 + individualSeed) - life;\\n        \\n        // Velocity + Gravity integration\\n        // Current Position = Origin + (Vel * t) + (0.5 * Gravity * t^2)\\n        // But since we want to move incrementally in a ping-pong buffer:\\n        vec3 vel = dir * speed * exp(-t * 0.5); // Air resistance\\n        vel.y += gravity * t;\\n        \\n        pos += vel * iTimeDelta;\\n        life -= iTimeDelta * decay;\\n    }\\n\\n    gl_FragColor = vec4(pos, life);\\n}\",\n  \"renderShader\": \"precision highp float;\\n\\nvarying float vLife;\\nvarying vec2 vUv;\\n\\nvoid main() {\\n    // Circular point shape\\n    float dist = distance(gl_PointCoord, vec2(0.5));\\n    if (dist > 0.5) discard;\\n    \\n    // Glow effect\\n    float strength = pow(1.0 - dist * 2.0, 2.0);\\n    \\n    // Color cycling based on UV (particle ID)\\n    vec3 colorA = vec3(1.0, 0.3, 0.1); // Orange/Red\\n    vec3 colorB = vec3(0.1, 0.6, 1.0); // Blue\\n    vec3 colorC = vec3(1.0, 0.8, 0.2); // Gold\\n    \\n    float colorPick = fract(vUv.x * 10.0 + vUv.y * 5.0);\\n    vec3 finalColor = colorA;\\n    if(colorPick > 0.33) finalColor = colorB;\\n    if(colorPick > 0.66) finalColor = colorC;\\n    \\n    // Sparkle effect\\n    float sparkle = sin(vLife * 20.0) * 0.5 + 0.5;\\n    \\n    // Final color with alpha fading\\n    gl_FragColor = vec4(finalColor + (sparkle * 0.2), vLife * strength);\\n}\",\n  \"vertexShader\": \"precision highp float;\\n\\nattribute vec2 uv;\\nuniform sampler2D tPosition;\\nuniform mat4 projectionMatrix;\\nuniform mat4 modelViewMatrix;\\n\\nvarying float vLife;\\nvarying vec2 vUv;\\n\\nvoid main() {\\n    vUv = uv;\\n    vec4 data = texture2D(tPosition, uv);\\n    vec3 pos = data.xyz;\\n    vLife = data.a;\\n\\n    vec4 mvPosition = modelViewMatrix * vec4(pos, 1.0);\\n    \\n    // Size gets smaller as life fades\\n    gl_PointSize = (15.0 * vLife) * (1