  "code": "/* PARAMS:\n[\n  {\"name\": \"speed\", \"label\": \"Flow Speed\", \"type\": \"float\", \"default\": 2.0, \"min\": 0.0, \"max\": 5.0},\n  {\"name\": \"scale\", \"label\": \"Grid Size\", \"type\": \"float\", \"default\": 1.5, \"min\": 0.5, \"max\": 4.0},\n  {\"name\": \"zoom\", \"label\": \"Camera Zoom\", \"type\": \"float\", \"default\": 1.0, \"min\": 0.5, \"max\": 3.0},\n  {\"name\": \"lineWidth\", \"label\": \"Line Width\", \"type\": \"float\", \"default\": 0.05, \"min\": 0.01, \"max\": 0.2},\n  {\"name\": \"alpha\", \"label\": \"Alpha Fade\", \"type\": \"float\", \"default\": 1.0, \"min\": 0.1, \"max\": 2.0}\n]\n*/\n\nlayout(location = 0) out vec4 pc_fragColor;\nuniform float iTime;\nuniform vec2 iResolution;\nuniform vec4 iMouse;\n\nuniform float speed;\nuniform float scale;\nuniform float zoom;\nuniform float lineWidth;\nuniform float alpha;\n\n#define MAX_STEPS 80\n#define MAX_DIST 20.0\n#define SURF_DIST 0.005\n\nmat2 rot(float a) {\n    float s = sin(a);\n    float c = cos(a);\n    return mat2(c, -s, s, c);\n}\n\n// GYROID SURFACE\nfloat getGyroid(vec3 p) {\n    p *= scale;\n    return dot(sin(p), cos(p.yzx));\n}\n\nfloat map(vec3 p) {\n    // Large Waving Motion\n    float wave = sin(p.z * 0.3 + iTime * speed * 0.5) * 1.0;\n    p.x += wave;\n    p.y += cos(p.z * 0.2 + iTime * speed * 0.3) * 1.0;\n    \n    // Get Gyroid Value\n    float g = getGyroid(p);\n    \n    // WIREFRAME LOGIC\n    // A solid gyroid is at \"g = 0\"\n    // To make a wireframe, we want the surface where abs(g) is small.\n    // But we actually want the INTERSECTION of surfaces (the \"tubes\").\n    // A cool wireframe trick is using the derivative or just slicing it thin.\n    \n    // Simple wireframe: shell thickness\n    float shell = abs(g) - lineWidth; \n    \n    // But let's do a dual lattice wireframe:\n    // This creates tubes where the gyroid surface intersects itself or near 0\n    // Actually, let's just render the surface as a thin shell, but accumulate transparently.\n    \n    // We return distance to surface\n    return abs(g) / scale * 0.5; // *0.5 to fix SDF Lipschitz\n}\n\nvoid main() {\n    vec2 uv = (gl_FragCoord.xy * 2.0 - iResolution.xy) / iResolution.y;\n    \n    // MOUSE\n    float yaw = 1.57; \n    float pitch = 0.0;\n    if (iMouse.z > 0.0 || iMouse.x > 0.0) {\n        yaw += (iMouse.x / iResolution.x - 0.5) * 6.0;\n        pitch += (iMouse.y / iResolution.y - 0.5) * 3.0;\n    } else {\n        yaw += iTime * 0.1; \n    }\n    \n    float tPos = iTime * speed;\n    float camDist = 5.0 / zoom;\n    \n    vec3 ro = vec3(0.0, 0.0, tPos);\n    ro.x += camDist * cos(pitch) * sin(yaw);\n    ro.y += camDist * sin(pitch);\n    ro.z += camDist * cos(pitch) * cos(yaw);\n    \n    vec3 ta = vec3(0, 0, tPos);\n    vec3 f = normalize(ta - ro);\n    vec3 r = normalize(cross(vec3(0,1,0),f));\n    vec3 u = cross(f,r);\n    vec3 rd = normalize(f + r*uv.x + u*uv.y);\n    \n    float t = 0.0;\n    float acc = 0.0;\n    \n    // RAYMARCHING for WIREFRAME\n    // We don't stop at the surface. We pass THROUGH it and add light.\n    for(int i = 0; i < MAX_STEPS; i++) {\n        vec3 p = ro + rd * t;\n        float d = map(p);\n        \n        // \"Hit\" if we are close to the wireframe shell\n        // Intensity curve: 1.0 / distance\n        float intensity = lineWidth / (abs(d) + 0.005);\n        \n        // Accumulate (Tone mapped)\n        acc += intensity * 0.02 * alpha;\n        \n        // Step forward.\n        // Important: Don't step too small or we accumulate infinite light.\n        // Don't step too large or we miss lines.\n        t += max(abs(d) * 0.7, 0.05); \n        \n        if(t > MAX_DIST) break;\n    }\n    \n    // COLOR\n    vec3 aqua = vec3(0.0, 1.0, 0.9);\n    vec3 blue = vec3(0.0, 0.2, 0.8);\n    \n    // Mix core vs outer glow\n    // High acc = Core (Aqua)\n    vec3 col = mix(blue, aqua, clamp(acc, 0.0, 1.0));\n    \n    col *= acc; // Total brightness\n    \n    // Fade out distance\n    col *= 1.0 / (1.0 + t * t * 0.01);\n    \n    pc_fragColor = vec4(col, 1.0);\n}",
