  "code": "\n\nout vec4 fragColor;\n\nuniform float iTime;\nuniform vec2 iResolution;\nuniform vec4 iMouse;\nuniform sampler2D iChannel0;\n\n// Uniforms from the JSON configuration\nuniform float u_speed;\nuniform float u_portal_radius;\nuniform float u_particle_count;\nuniform float u_flare_intensity;\nuniform float u_chromatic;\nuniform float u_hue_shift;\nuniform float u_distortion;\n\n#define PI 3.14159265359\n\n// Hash function for pseudo-randomness\nfloat hash(float n) { return fract(sin(n) * 43758.5453123); }\n\n// Rotation matrix\nmat2 rot(float a) {\n    float s = sin(a), c = cos(a);\n    return mat2(c, -s, s, c);\n}\n\n// 3D Noise for volumetric texture\nfloat noise(vec3 p) {\n    vec3 i = floor(p);\n    vec3 f = fract(p);\n    f = f * f * (3.0 - 2.0 * f);\n    float n = i.x + i.y * 57.0 + 113.0 * i.z;\n    return mix(mix(mix(hash(n + 0.0), hash(n + 1.0), f.x),\n                   mix(hash(n + 57.0), hash(n + 58.0), f.x), f.y),\n               mix(mix(hash(n + 113.0), hash(n + 114.0), f.x),\n                   mix(hash(n + 170.0), hash(n + 171.0), f.x), f.y), f.z);\n}\n\n// Color palette function\nvec3 palette(float t) {\n    vec3 a = vec3(0.5, 0.5, 0.5);\n    vec3 b = vec3(0.5, 0.5, 0.5);\n    vec3 c = vec3(1.0, 1.0, 1.0);\n    vec3 d = vec3(0.0, 0.33, 0.67);\n    return a + b * cos(6.28318 * (c * t + d + u_hue_shift));\n}\n\n// Raymarching function (Updated to remove the solid sphere/ball)\nfloat map(vec3 p) {\n    // We removed the sphere intersection. \n    // Instead, we return a value that allows raymarching to handle lensing \n    // without hitting a solid surface in the center.\n    return length(p) - 0.001; \n}\n\nvoid main() {\n    vec2 uv = (gl_FragCoord.xy - 0.5 * iResolution.xy) / iResolution.y;\n    float time = iTime * u_speed;\n    \n    // Camera setup\n    vec3 ro = vec3(0.0, 0.0, -4.0);\n    vec3 rd = normalize(vec3(uv, 1.5));\n    \n    // Rotate camera based on mouse\n    float mouseX = (iMouse.x / iResolution.x - 0.5) * 2.0;\n    float mouseY = (iMouse.y / iResolution.y - 0.5) * 2.0;\n    rd.yz *= rot(mouseY * 0.2);\n    rd.xz *= rot(mouseX * 0.5);\n    \n    vec3 finalColor = vec3(0.0);\n    float totalDist = 0.0;\n    \n    // 1. GRAVITATIONAL LENSING\n    // This loop now purely bends the light without terminating on a \"ball\" surface\n    for(int i = 0; i < 40; i++) {\n        vec3 p = ro + rd * totalDist;\n        float d = length(p); // Distance to singularity\n        \n        // Lens bending logic: rays curve toward the singularity\n        // u_distortion controls how much the background and particles warp\n        float force = 0.01 * u_distortion / (d * d + 0.1);\n        rd = normalize(rd - normalize(p) * force);\n        \n        totalDist += max(0.01, d * 0.2);\n        if(totalDist > 10.0) break;\n    }\n    \n    // Background Space / Nebula\n    vec3 bg = palette(length(uv) * 0.1 + time * 0.05) * 0.1;\n    bg += pow(noise(rd * 2.0 + time * 0.1), 8.0) * palette(0.6);\n    finalColor = bg;\n\n    // 2. NEON PARTICLES (Physics-based orbits)\n    for(float i = 0.0; i < 1.0; i += 1.0/100.0) {\n        if(i * 200.0 > u_particle_count) break;\n        \n        float h = hash(i * 123.456);\n        float pSpeed = (0.5 + h) * 2.0;\n        float radius = u_portal_radius + h * 0.5;\n        float angle = time * pSpeed + h * PI * 2.0;\n        \n        // Orbital positions\n        vec3 pos = vec3(\n            cos(angle) * radius,\n            sin(angle * 0.8) * radius * 0.5,\n            sin(angle) * radius\n        );\n        \n        // Flare/Streak calculation\n        vec3 pToCam = pos - ro;\n        float projection = dot(pToCam, rd);\n        vec3 closestPoint = ro + rd * projection;\n        float distToParticle = length(closestPoint - pos);\n        \n        // Physics-based glow falloff\n        float glow = 0.002 / (distToParticle + 0.001);\n        glow *= smoothstep(10.0, 0.0, length(pToCam)); // Distance fade\n        \n        // Sparkle/Flicker\n        float flicker = smoothstep(0.4, 1.0, noise(vec3(i * 10.0, time * 5.0, 0.0)));\n        \n        vec3 pCol = palette(i + time * 0.1);\n        finalColor += pCol * glow * (1.0 + flicker * 2.0) * u_flare_intensity;\n        \n        // Spin Flare Streaks\n        float flare = pow(max(0.0, 1.0 - distToParticle * 8.0), 16.0);\n        finalColor += pCol * flare * 0.5 * u_flare_intensity;\n    }\n    \n    // 3. EVENT HORIZON GLOW (Soft center, no solid ball)\n    float distToCenter = length(uv);\n    float coreGlow = exp(-distToCenter * 4.0 / u_portal_radius);\n    vec3 coreColor = palette(time * 0.2);\n    finalColor += coreColor * coreGlow * u_flare_intensity;\n    \n    // 4. POST-PROCESSING\n    // Vignette\n    finalColor *= 1.0 - dot(uv, uv) * 0.5;\n    \n    // Chromatic Aberration\n    finalColor.r = mix(finalColor.r, texture(iChannel0, uv * (1.0 + u_chromatic)).r, 0.05);\n    finalColor.b = mix(finalColor.b, texture(iChannel0, uv * (1.0 - u_chromatic)).b, 0.05);\n    \n    // HDR Tonemapping (Reinhard)\n    finalColor = finalColor / (finalColor + vec3(1.0));\n    \n    // Gamma correction\n    finalColor = pow(finalColor, vec3(0.4545));\n\n    fragColor = vec4(finalColor, 1.0);\n}",
