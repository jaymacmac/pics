  "code": "\n\nuniform float iTime;\nuniform vec2 iResolution;\nuniform vec4 iMouse;\nuniform sampler2D iChannel0;\n\n// User Parameters\nuniform float u_speed;\nuniform float u_glow_intensity;\nuniform float u_complexity;\nuniform float u_camera_dist;\n\nout vec4 fragColor;\n\n// --- UTILS ---\nmat2 rot(float a) {\n    float s = sin(a), c = cos(a);\n    return mat2(c, -s, s, c);\n}\n\n// Smooth Minimum for organic blending\nfloat smin(float a, float b, float k) {\n    float h = clamp(0.5 + 0.5 * (b - a) / k, 0.0, 1.0);\n    return mix(b, a, h) - k * h * (1.0 - h);\n}\n\n// 3D Noise function for volumetric density\nfloat noise(vec3 p) {\n    vec3 i = floor(p);\n    vec3 f = fract(p);\n    f = f * f * (3.0 - 2.0 * f);\n    \n    // Sampling iChannel0 as a noise source (blue noise or white noise)\n    vec2 uv = (i.xy + vec2(37.0, 17.0) * i.z) + f.xy;\n    float rg = texture(iChannel0, (uv + 0.5) / 256.0).r;\n    return mix(rg, texture(iChannel0, (uv + 128.5) / 256.0).r, f.z);\n}\n\n// --- CORE LOGIC ---\nvec2 getPath(float z) {\n    return vec2(\n        sin(z * 0.12) * 3.5 + cos(z * 0.05) * 2.0,\n        cos(z * 0.15) * 2.5 + sin(z * 0.08) * 1.5\n    );\n}\n\nfloat map(vec3 p) {\n    // Subtract path to center the \"universe\" around the moving camera\n    p.xy -= getPath(p.z);\n    \n    vec3 q = p;\n    float t = iTime * 0.2;\n    \n    // Infinite Domain Repetition with smooth twisting\n    q.xy *= rot(q.z * 0.05 + t);\n    \n    // Create large, soft SDF \"blobs\" for the gradients to cling to\n    float d = 1e10;\n    \n    for(float i = 0.0; i < 3.0; i++) {\n        float shift = i * 2.1;\n        vec3 p_i = q;\n        p_i.xy *= rot(t * 0.5 + shift);\n        p_i.y += sin(q.z * 0.2 + shift) * 2.0;\n        \n        // Primitive shapes\n        float sphere = length(p_i.xy) - (1.5 + sin(q.z * 0.1 + i) * 0.5);\n        d = smin(d, sphere, 2.5);\n    }\n    \n    // Add fine-scale noise displacement for \"smoke\" effect\n    d += noise(p * 0.4 + iTime * 0.1) * 0.8;\n    \n    return d * 0.6; // Step down for raymarching stability\n}\n\nvoid main() {\n    vec2 uv = (gl_FragCoord.xy - 0.5 * iResolution.xy) / iResolution.y;\n    \n    // 1. Setup Camera Motion\n    float t = iTime * u_speed;\n    vec3 pathPos = vec3(0.0, 0.0, t);\n    pathPos.xy += getPath(pathPos.z);\n    \n    // Parallel Rail Strategy: Camera orbits the central path\n    float railOffset = u_camera_dist;\n    vec2 railVec = vec2(cos(t * 0.1), sin(t * 0.1)) * railOffset;\n    \n    vec3 ro = pathPos;\n    ro.xy += railVec;\n    \n    // Target follows the path ahead but stays offset to keep perspective\n    vec3 ta = vec3(0.0, 0.0, t + 10.0);\n    ta.xy += getPath(ta.z) + railVec * 0.5;\n    \n    // 2. Camera Basis\n    vec3 cw = normalize(ta - ro);\n    vec3 cp = vec3(0.0, 1.0, 0.0);\n    vec3 cu = normalize(cross(cw, cp));\n    vec3 cv = normalize(cross(cu, cw));\n    vec3 rd = normalize(uv.x * cu + uv.y * cv + 1.5 * cw);\n    \n    // 3. Raymarching / Volumetric Accumulation\n    vec3 col = vec3(0.0);\n    float dSum = 0.0;\n    float t_dist = 0.0;\n    \n    // Aesthetic Gradient Colors\n    vec3 colorA = vec3(0.1, 0.4, 0.9); // Deep Blue\n    vec3 colorB = vec3(0.9, 0.2, 0.6); // Vibrant Magenta\n    vec3 colorC = vec3(0.0, 0.9, 0.7); // Cyan Teal\n    \n    for(int i = 0; i < 80; i++) {\n        vec3 p = ro + rd * t_dist;\n        float d = map(p);\n        \n        // Volumetric Glow Accumulation\n        // We use a \"soft\" distance check to accumulate color in the fog\n        float val = smoothstep(4.0, -1.0, d);\n        if (val > 0.0) {\n            // Dynamic color mixing based on Z-depth and local noise\n            float hue = fract(p.z * 0.02 + noise(p * 0.1));\n            vec3 localCol = mix(colorA, colorB, smoothstep(0.0, 0.5, hue));\n            localCol = mix(localCol, colorC, smoothstep(0.5, 1.0, hue));\n            \n            // Apply exponential falloff for smooth fading\n            float density = val * exp(-t_dist * 0.08);\n            col += localCol * density * u_glow_intensity;\n        }\n        \n        t_dist += max(0.1, abs(d) * 0.7);\n        if (t_dist > 40.0) break;\n    }\n    \n    // 4. Post-Processing\n    // Subtle vignette\n    col *= 1.0 - length(uv) * 0.4;\n    \n    // Tone mapping and Gamma correction\n    col = pow(col, vec3(0.8)); // Punchy contrast\n    col = smoothstep(0.0, 1.1, col); // Soft clip\n    \n    fragColor = vec4(col, 1.0);\n}",
