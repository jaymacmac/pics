  "code": "\n\nuniform float iTime;\nuniform vec2 iResolution;\nuniform vec4 iMouse;\nuniform sampler2D iChannel0;\n\n// User Defined Parameters\nuniform float u_speed;\nuniform float u_complexity;\nuniform float u_transparency;\nuniform float u_glow;\n\nout vec4 fragColor;\n\n// Rotation matrix\nmat2 rot(float a) {\n    float s = sin(a), c = cos(a);\n    return mat2(c, -s, s, c);\n}\n\n// 3D Noise function for subtle volumetric variation\nfloat hash(vec3 p) {\n    p  = fract(p * 0.1031);\n    p += dot(p, p.zyx + 31.32);\n    return fract((p.x + p.y) * p.z);\n}\n\n// The Gyroid formula: sin(x)cos(y) + sin(y)cos(z) + sin(z)cos(x)\nfloat sdGyroid(vec3 p, float scale, float thickness, float bias) {\n    p *= scale;\n    return abs(dot(sin(p), cos(p.zxy)) - bias) / scale - thickness;\n}\n\n// Scene mapping\nfloat map(vec3 p) {\n    // Large scale structural gyroid\n    float g1 = sdGyroid(p, 0.4, 0.03, 0.0);\n    \n    // Medium detail gyroid layer\n    float g2 = sdGyroid(p, 1.2, 0.02, 0.5 * sin(iTime * 0.2));\n    \n    // Combine for a complex \"infinite scaffold\" look\n    return max(g1, -g2 * 0.8);\n}\n\n// Smooth min for color blending\nfloat smin(float a, float b, float k) {\n    float h = clamp(0.5 + 0.5 * (b - a) / k, 0.0, 1.0);\n    return mix(b, a, h) - k * h * (1.0 - h);\n}\n\nvoid main() {\n    // Aspect correct UVs\n    vec2 uv = (gl_FragCoord.xy - 0.5 * iResolution.xy) / iResolution.y;\n    \n    // Slow drift movement\n    float t = iTime * u_speed;\n    vec3 ro = vec3(0.0, 1.5, t); // Camera origin\n    ro.xy += vec2(sin(t * 0.1), cos(t * 0.15)) * 2.0; // Subtle sway\n    \n    // Look-at logic (towards horizon)\n    vec3 lookAt = vec3(0.0, 0.0, t + 5.0);\n    lookAt.xy += vec2(sin((t + 5.0) * 0.1), cos((t + 5.0) * 0.15)) * 2.0;\n    \n    vec3 forward = normalize(lookAt - ro);\n    vec3 right = normalize(cross(vec3(0.0, 1.0, 0.0), forward));\n    vec3 up = cross(forward, right);\n    \n    // Ray direction\n    vec3 rd = normalize(forward + uv.x * right + uv.y * up);\n    rd.xy *= rot(sin(t * 0.05) * 0.2); // Gentle roll\n    \n    // Volumetric Raymarching (for transparency/glow)\n    vec3 col = vec3(0.0);\n    float accumulation = 0.0;\n    float d = 0.0;\n    float depth = 0.05;\n    \n    // Dithering to reduce banding\n    depth += hash(vec3(uv * iResolution.y, iTime)) * 0.05;\n\n    for(int i = 0; i < 80; i++) {\n        vec3 p = ro + rd * depth;\n        float dist = map(p);\n        \n        // If we are close to the gyroid surface\n        if (dist < 0.1) {\n            // Create a glow effect based on proximity to the surface\n            float filterGlow = pow(clamp(1.0 - abs(dist) / 0.1, 0.0, 1.0), 4.0);\n            \n            // Tint based on depth and position\n            vec3 edgeColor = 0.5 + 0.5 * cos(vec3(0.0, 1.0, 2.0) + p.z * 0.2 + iTime * 0.1);\n            \n            // Accumulate transparency\n            float alpha = (1.0 - dist / 0.1) * u_transparency;\n            col += edgeColor * filterGlow * alpha * u_glow;\n            accumulation += alpha;\n        }\n        \n        // Step forward. Since it's transparent, we don't break, we just advance.\n        depth += max(dist * 0.5, 0.02);\n        \n        if (depth > 20.0 || accumulation > 1.0) break;\n    }\n\n    // Horizon fog/background\n    vec3 horizonColor = vec3(0.02, 0.05, 0.1) * (1.0 - length(uv) * 0.5);\n    col = mix(horizonColor, col, clamp(accumulation, 0.0, 1.0));\n    \n    // Bloom/Post-process\n    col += pow(col, vec3(2.2)) * 0.5;\n    \n    // Output\n    fragColor = vec4(col, 1.0);\n}",
